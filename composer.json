{
  "name": "florianajir/home-api",
  "description": "PHP Client for florianajir API",
  "keywords": [
    "florianajir",
    "api",
    "client",
    "php"
  ],
  "type": "library",
  "homepage": "https://github.com/florianajir/home-api",
  "license": "MIT",
  "authors": [
    {
      "name": "LoÃ¯c Ambrosini",
      "email": "loic@florianajir.com"
    }
  ],
  "require": {
      "php":             ">=5.4",
      "ext-curl":        "*",
      "guzzle/guzzle":   "~3.7"
  },
  "require-dev": {
      "phpunit/phpunit":            "~4.0",
      "squizlabs/php_codesniffer":  "2.*",
      "sebastian/phpcpd":           "2.*",
      "phpmd/phpmd" :               "@stable",
      "phing/phing":                "~2.9"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
  },
  "autoload": {
      "psr-0": { "Meup\\Api\\Client\\": "src/" }
  },
  "config": {
      "bin-dir": "./bin"
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.0-dev"
      }
  }
}
